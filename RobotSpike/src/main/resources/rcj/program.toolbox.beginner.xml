<toolbox_set id="toolboxBeginner" style="display: none">
    <category name="TOOLBOX_ACTION" svg="true">
        <block type="actions_motorDiff_on_for">
            <value name="POWER">
                <block type="math_number">
                    <field name="NUM">30</field>
                </block>
            </value>
            <value name="DISTANCE">
                <block type="math_number">
                    <field name="NUM">10</field>
                </block>

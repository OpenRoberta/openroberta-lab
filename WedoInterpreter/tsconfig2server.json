{
    "compilerOptions": {
        "outDir": "../OpenRobertaServer/staticResources/js/app/wedoInterpreter",
        "target": "es5",
        "rootDir": "./ts",
        "typeRoots" : ["./node_modules/@types"],
        "noUnusedLocals": true,
        
        "module": "amd",
        "baseUrl": "./ts",
        "paths": {
            "interpreter.*": ["*"]
        },
		"noUnusedParameters": true,
		"allowJs": true
    },
    "include": ["ts/**/*"],
    "exclude": [
        "**/node_modules",
        "**/interpreter.runStackMachineJson.ts"
    ],
    "compileOnSave": false,
    "buildOnSave": false
}